#include<stdio.h>
int main()
{
	int n;
	scanf("%d",&n);
	int ja[n],ba[n];
	char id[n][17];//16+1
	int i=0;
	for(i=0;i<n;i++)
	{
		scanf("%s%d%d",&id[i],&ja[i],&ba[i]);
	}
	int m;
	scanf("%d",&m);
	int s[m];
	for(i=0;i<m;i++)
	{
		scanf("%d",&s[i]);
	}
	int j;
	for(i=0;i<m;i++)
	{
		int index;
		for(j=0;j<n;j++)
		{
			if(s[i]==ja[j])
			{
				index=j;
				break;

			}
		}
		printf("%s %d\n",in[index],ba[index]);
	}
	return 0;
}
